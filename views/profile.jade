extends layout

block content
	.container
		.page-header.text.center
			h1.text-white
				span.fa.fa-anchor Profile Page
		.row
			.col-md-8.col-md-offset-2
				.well
					h3
						span.fa.fa-user User Data
					p
						strong id:
						| #{user._id}
						br
						strong email:
						| #{user.local.email}
						br
						strong password:
						| #{user.local.password}