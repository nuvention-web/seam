	body.bg-gray
		#wrapper
			nav.navbar.navbar-inverse.navbar-fixed-top.bg-white.border-white(role='navigation')
				.navbar-header
					button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-ex1-collapse')
						span.sr-only Toggle navigation
						span.icon-bar
						span.icon-bar
				.collapse.navbar-collapse.navbar-ex1-collapse
					ul.nav.navbar-nav.side-nav.bg-white.text-left
						img.img-responsive(style="margin-left:25px; width:150px; height:180px", src="images/logo.png")
						li.text-left.text-h5.margin-top-20p
							a.text-left(href='/dashboard')
								i.fa.fa-home.margin-left-15x.text-h5.text-black
								i.text-no-transform.margin-left-15x.text-h5.text-black HOME
						li
							a.text-left(href='/dashboard/meetings/makeMeeting')
								i.fa.fa-pencil.margin-left-15x.text-h5.text-black
								i.text-no-transform.margin-left-15x.text-h5.text-black CREATE
						li
							a.text-left(href='/dashboard/meetings/pastMeeting')
								i.fa.fa-clock-o.margin-left-15x.text-h5.text-black
								i.text-no-transform.margin-left-15x.text-h5.text-black MINUTES
						li
							a.text-left(href='/dashboard/tasks')
								i.fa.fa-check-square.margin-left-15x.text-h5.text-black
								i.text-no-transform.margin-left-15x.text-h5.text-black TASKS
					ul.nav.navbar-nav.navbar-right.navbar-user
						li.text-left.text-h5.margin-right-15x
							h5.text-left.text-pink.text-normal.text-upper(style="margin-top:17px" href='#') #{name}
						//-li.text-left.text-h5
							//-a.text-left.text-gray.text-h5(href='/help') HELP
						//-li.text-left.text-h5
							//-a.text-left.text-gray.text-h5(href='/settings') SETTINGS
						li.text-left.text-h5.margin-right-15x
							a.text-left.text-black-nohover.text-h5(href='/logout') LOGOUT
			#page-wrapper.bg-gray.margin-top-1p
				block content
